{
  "name": "fullstack-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "debug:client": "vite",
    "debug:server:dev": "cd src/backend && ENV=DEVELOPMENT tsx watch --inspect=0.0.0.0:9229 app.ts",
    "debug:server:prod": "cd src/backend && ENV=PRODUCTION tsx watch --inspect=0.0.0.0:9229 app.ts",
    "debug:all:dev": "npm-run-all --parallel debug:server:dev debug:client",
    "debug:all:prod": "npm-run-all --parallel debug:server:prod debug:client",

    "build:client": "tsc -b tsconfig.app.json && vite build",
    "build:server:dev": "tsc -b tsconfig.node.json && ENV=DEVELOPMENT node dist/server/app.js",

    "prisma:generate": "npx prisma generate",
    "prisma:deploy:dev": "npx dotenv -e .env.migration.development -- prisma migrate dev",
    "prisma:deploy:prod": "npx dotenv -e .env.migration.production -- prisma migrate deploy"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@aws-sdk/client-cloudfront": "^3.716.0",
    "@aws-sdk/client-s3": "^3.717.0",
    "@aws-sdk/cloudfront-signer": "^3.734.0",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.10.1",
    "@tailwindcss/vite": "^4.1.10",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^5.0.1",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "luxon": "^3.6.1",
    "mime-types": "^3.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.0",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "performant-array-to-tree": "^1.11.0",
    "pg": "^8.14.1",
    "pg-native": "^3.3.0",
    "rate-limit-redis": "^4.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "react-virtuoso": "^4.13.0",
    "socket.io": "^4.8.1",
    "tailwindcss": "^4.1.10",
    "ts-enum-util": "^4.1.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/ioredis": "^4.28.10",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/luxon": "^3.6.2",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.9.0",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.11.13",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/winston": "^2.4.4",
    "@vitejs/plugin-react": "^4.4.1",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
